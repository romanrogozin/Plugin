{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AuthorizationParameters",
  "type": "object",
  "additionalProperties": false,
  "definitions": {
    "UserProfession": {
      "type": "integer",
      "description": "",
      "x-enumNames": [ "None", "Bot", "InternalTester", "Developer", "InternalPlayTester", "BnetTester", "BnetPlayer", "SteamTester", "SteamPlayer", "XboxTester", "XboxPlayer", "PsnTester", "PsnPlayer", "EpicTester", "EpicPlayer", "FirebaseTester", "FirebasePlayer", "Player" ],
      "enum": [ 0, 1, 3, 5, 7, 13, 19, 23, 29, 33, 39, 43, 49, 53, 59, 63, 69, 255 ]
    },
    "AccountFlagRule": {
      "type": "integer",
      "description": "",
      "x-enumFlags": true,
      "x-enumNames": [ "None", "SignIn", "Create", "Allow" ],
      "enum": [ 0, 1, 2, 3 ]
    },
    "AuthorizationFactProcessing": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Enabled": {
          "type": "boolean",
          "default": true
        },
        "Finalize": {
          "type": "boolean",
          "default": true
        },
        "Stage": {
          "type": "boolean",
          "default": true
        },
        "Delay": {
          "type": "string",
          "format": "time-span"
        }
      }
    }
  },
  "properties": {
    "CompatibilityCheckDisabled": {
      "type": "boolean",
      "default": false
    },
    "SoftCheckEnabled": {
      "type": "boolean",
      "default": false
    },
    "ClientAddressValidation": {
      "type": "boolean",
      "default": true
    },
    "GameClientValidationEnabled": {
      "type": "boolean",
      "default": true
    },
    "UseSoftAclPolicy": {
      "type": "boolean",
      "default": false 
    },
    "CurrentPersistanceVersion": {
      "type": "integer",
      "format": "int32",
      "default": 0 
    },
    "CurrentImportVersion": {
      "type": "integer",
      "format": "int32",
      "default": 0 
    },
    "AutoImportEnabled": {
      "type": "boolean",
      "default": false 
    },
    "AutoImportUri": {
      "type": [ "string" ],
      "default": "" 
    },
    "ForcePlayerGroupWhenNoGroupsAvailable": {
      "type": [ "string" ],
      "default" :  "" 
    },
    "SignInExpiration": {
      "type": "string",
      "format": "time-span"
    },
    "TokenProlongation": {
      "type": "integer",
      "format": "int32",
      "default": 85 
    },
    "WorkerSleepTimeout": {
      "type": "string",
      "format": "time-span",
      "description": "Sleep interval if backend is full"
    },
    "SignInTimeouts": {
      "type": [ "null", "object" ],
      "x-dictionaryKey": {
        "type": "integer",
        "format": "int32"
      },
      "additionalProperties": {
        "type": "integer",
        "format": "int32"
      }
    },
    "Capacity": {
      "type": [ "integer", "null" ],
      "format": "int32"
    },
    "SignInCapacityLimiterEnabled": {
      "type": "boolean",
      "default": false
    },
    "CheckSignatureTimeOffset": {
      "type": "boolean",
      "default": false 
    },
    "DegreeOfParallelism": {
      "type": "integer",
      "format": "int32",
      "default": 4 
    },
    "QueueReaderSleepTimeout": {
      "type": "string",
      "format": "time-span",
      "description": "Sleep interval if queue is empty"
    },
    "MaxSignatureTimeOffset": {
      "type": "integer",
      "format": "int32",
      "default": 300 
    },
    "EnableGeoIpResolve": {
      "type": "boolean",
      "default": false 
    },
    "RetailBuilds": {
      "type": [ "array" ],
      "items": {
        "type": "string"
      }
    },
    "LiveBackend": {
      "type": "boolean",
      "default": false
    },
    "AllowedProfessions": {
      "type": [ "null", "object" ],
      "x-dictionaryKey": {
        "$ref": "#/definitions/UserProfession"
      },
      "additionalProperties": {
        "$ref": "#/definitions/AccountFlagRule"
      }
    },
    "PriorityQueueForNonplayers": {
      "type": "boolean",
      "default": false
    },
    "CommonQueueProfession": {
      "$ref": "#/definitions/UserProfession",
      "default": "Player"
    },
    "F2Pmode": {
      "$ref": "#/definitions/AccountFlagRule",
      "default": "None"
    },
    "MinTokenRefreshAfterMinutes": {
      "type": "integer",
      "format": "int32",
      "default": "5"
    },
    "SignInRateLimiterMaxRequestCount": {
      "type": "integer",
      "format": "int32",
      "default": "200"
    },
    "SignInRateLimiterInterval": {
      "type": "string",
      "format": "time-span"
    },
    "AuthFactProcessingOnSignout": {
      "oneOf": [
        {
          "$ref": "#/definitions/AuthorizationFactProcessing"
        }
      ]
    },
    "DefaultNicknamePerProviders": {
      "type": [ "array" ],
      "items": {
        "type": "string"
      }
    },
    "DefaultNickname": {
      "type": [ "null", "string" ],
      "default": ""
    },
    "DelayOnRetryMs": {
      "type": "integer",
      "format": "int32",
      "default": "7000"
    }
  },
  "required": [ "DefaultNicknamePerProviders", "AuthFactProcessingOnSignout"]
}